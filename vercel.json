{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "version": 2,
    "devCommand": "yarn dev",
    "installCommand": "yarn install",
    "buildCommand": "yarn build",
    "outputDirectory": ".",
    "public": false,
    "framework": null,
		"functions": {

			"api/*.js": {
				"memory": 1024,
				"maxDuration": 10
			},

			"api/**/*.js": {
				"memory": 1024,
				"maxDuration": 10
			},

			"api/**/*.ts": {
				"memory": 1024,
				"maxDuration": 10
			}
		},
		"git": {
		"deploymentEnabled": true
	},
	"github": {
		"enabled": true,
		"silent": false
	},
	"headers": [
		{
			"source": "/(.html)",
			"headers": [
				{
					"key": "X-UA-Compatible",
					"value": "ie-edge"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
          "key": "Cache-Control",
          "value": "public, max-age=10, s-maxage=86400, immutable"
        },
				{
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
			]
		},
		{
			"source": "github-readme-stats.stoneydsp.com/(.*)",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
          "key": "Cache-Control",
          "value": "public, max-age=10, s-maxage=86400, immutable"
        }
			]
		},
		{
			"source": "skillicons.dev/",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
          "key": "Cache-Control",
          "value": "public, max-age=10, s-maxage=86400, immutable"
        }
			]
		},
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }

      ]
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "query",
          "key": "authorized"
        }
      ],
      "headers": [
        {
          "key": "x-authorized",
          "value": "true"
        }
      ]
    }
  ],
	"redirects": [
		{
			"source": "/.well-known/",
			"destination": "/index.html"
		},
		{
			"source": "/backup/",
			"destination": "/index.html"
		},
		{
			"source": "/blog/",
			"destination": "/index.html"
		},
		{
			"source": "/new/",
			"destination": "/index.html"
		},
		{
			"source": "/old/",
			"destination": "/index.html"
		},
		{
			"source": "/temp/",
			"destination": "/index.html"
		},
		{
			"source": "/test/",
			"destination": "/index.html"
		},
		{
			"source": "/wordpress/",
			"destination": "/index.html"
		},
    {
			"source": "/wp/",
			"destination": "/index.html"
		},
		{
			"source": "/wp-admin/",
			"destination": "/index.html"
		},
		{
			"source": "/wp-content/",
			"destination": "/index.html"
		},
		{
			"source": "/wp-includes/",
			"destination": "/index.html"
		},
		{
			"source": "/wp-login.php",
			"destination": "/index.html"
		}
	]
}
